        <!--企业邮箱-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/biankuang_dark"
            android:paddingBottom="3dp"
            android:paddingLeft="10dp"
            android:paddingRight="5dp"
            android:paddingTop="3dp">

            <TextView

                android:id="@+id/register_company_text_emile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center_horizontal"
                android:text="企 业 邮 箱"
                android:textSize="@dimen/dm028" />

            <ImageView
                android:background="#999999"
                android:layout_marginLeft="10dp"
                android:layout_width="1dp"
                android:id="@+id/register_company_iv_emile_line"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/register_company_text_emile"
                android:src="@drawable/login_vertival_line" />

            <EditText
                android:id="@+id/register_company_editer_emile_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/register_company_iv_emile_line"
                android:background="@null"
                android:hint="请输入电子邮箱"
                android:singleLine="true"
                android:textColorHint="@color/gray"
                android:textSize="@dimen/dm028" />
        </RelativeLayout>

        <!--企业名称-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/biankuang_dark"
            android:paddingBottom="3dp"
            android:paddingLeft="10dp"
            android:paddingRight="5dp"
            android:paddingTop="3dp">

            <TextView
                android:id="@+id/register_company_text_organization_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="企 业 名 称"
                android:textSize="@dimen/dm028" />

            <ImageView
                android:background="#999999"
                android:layout_marginLeft="10dp"
                android:id="@+id/register_company_iv_organization_line"
                android:layout_width="1dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/register_company_text_organization_code"
                android:src="@drawable/login_vertival_line" />

            <EditText
                android:id="@+id/register_company_editer_organization_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/register_company_iv_organization_line"
                android:background="@null"
                android:hint="请输入企业名称"
                android:singleLine="true"
                android:textColorHint="@color/gray"
                android:textSize="@dimen/dm028" />
        </RelativeLayout>

        <Button
            android:id="@+id/register_company_btn_submit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/login_btn_blue"
            android:text="完成"
            android:textColor="#ffffff"
            android:textSize="@dimen/dm032" />
    </LinearLayout>


</LinearLayout>
